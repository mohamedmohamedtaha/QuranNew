<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/style_default_layout_match_parent"
    tools:context=".ui.activities.ElarbaoonElnawawyActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/ElarbaoonElnawawyActivity_TB"
        style="@style/style_toolbar" />

    <TextView
        android:id="@+id/ElarbaoonElnawawyActivity_TV_Elarbaoon_Elnawawy"
        style="@style/style_default_layout_wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/dimens_7xl"
        android:layout_marginTop="@dimen/dimens_l"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textColor="@android:color/white"
        android:textSize="@dimen/txt_size_l"
        android:textStyle="bold" />

    <com.miguelcatalan.materialsearchview.MaterialSearchView
        android:id="@+id/ElarbaoonElnawawyActivitySearch_View"
        style="@style/style_default_layout_wrap_match" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ElarbaoonElnawawyActivity_RecyclerView"
        style="@style/style_reycler_view_elnawawy"
        android:layout_below="@+id/ElarbaoonElnawawyActivity_TB"
        android:layout_gravity="center_horizontal"
        android:clickable="true"
        android:focusable="true"
        android:layoutDirection="ltr" />

    <ProgressBar
        android:id="@+id/ElarbaoonElnawawyActivity_ProgressBar"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:indeterminateDrawable="@drawable/color_progressbar"
        android:visibility="gone" />

    <TextView
        android:id="@+id/ElarbaoonElnawawyActivity_TV_No_Data"
        style="@style/style_text_view_no_data" />
</RelativeLayout>