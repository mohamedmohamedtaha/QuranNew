<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/style_default_layout_match_parent"
    tools:context=".ui.navigationview.ui.ElarbaoonElnawawyActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/elarbaoonElnawawyActivityTB"
        style="@style/style_toolbar" />

    <TextView
        android:id="@+id/ElarbaoonElnawawyActivity_TV_Elarbaoon_Elnawawy"
        style="@style/style_default_layout_wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/dimens_7xl"
        android:layout_marginTop="@dimen/dimens_l"

        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textColor="@android:color/white"
        android:textSize="@dimen/txt_size_l"
        android:textStyle="bold" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerViewDrawable"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/elarbaoonElnawawyActivityTB"
        app:defaultNavHost="true"
        app:navGraph="@navigation/nav_graph_drawable" />
